{
  "name": "w3susers",
  "version": "1.0.0",
  "description": "This is a simple website using interfacing with a  ColdFusion API",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "echo \"site is working\"",
    "start": "nodemon app.js",
    "client": "npm run dev --prefix frontend",
    "server": "nodemon app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "keywords": [
    "ColdFusion"
  ],
  "author": "MagicAces",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "nodemon": "^3.0.1",
    "soap": "^1.0.0"
  }
}
